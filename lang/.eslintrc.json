{
  "root": true,
  "extends": ["./node_modules/sanctuary-style/eslint-es6.json"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "script"
  },
  "env": {"node": true},
  "rules": {
    "max-len": ["off"],
    "quotes": ["error", "single", {"allowTemplateLiterals": true, "avoidEscape": true}]
  }
}
