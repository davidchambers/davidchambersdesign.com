import * as path from "https://deno.land/std@0.182.0/path/mod.ts";

export {
  basename,
  dirname,
  join,
  relative,
};

basename path_ suffix = {
  apply path.basename [path_, suffix]
};

dirname = {
  path.dirname
};

join = {
  apply path.join
};

relative from to = {
  apply path.relative [from, to]
};
