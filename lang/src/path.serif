import * as path from "https://deno.land/std@0.182.0/path/mod.ts";

export {
  basename,
  dirname,
  join,
  relative,
};

basename path_ suffix = (
  path.basename (path_, suffix)
);

dirname = (
  path.dirname
);

join components = (
  path.join (...components)
);

relative from to = (
  path.relative (from, to)
);
