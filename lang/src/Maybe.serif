export {
  Nothing,
  Just,
  maybe,
  fromMaybe,
  fromNullable,
};

type Maybe = Nothing | Just value implements {
  fantasy-land/map: \f -> case this of [
    Nothing -> Nothing,
    Just x -> Just $ f x,
  ],
};

maybe y f m = case m of [Nothing -> y, Just x -> f x];

fromMaybe default = maybe default id;

fromNullable x = if x == null || x == undefined then Nothing else Just x;
