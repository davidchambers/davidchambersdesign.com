import {attemptP} from "fluture";

export {
  mkdir,
  readFile,
  writeFile,
};

mkdir options path = {
  attemptP \() -> apply Deno.mkdir [path, options]
};

readFile filename = {
  attemptP \() -> Deno.readTextFile filename
};

writeFile filename data = {
  attemptP \() -> apply Deno.writeTextFile [filename, data]
};
