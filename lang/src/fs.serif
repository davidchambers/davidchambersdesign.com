import {attemptP} from "fluture";

export {
  mkdir,
  readFile,
  writeFile,
};

mkdir options path = (
  attemptP \() -> Deno.mkdir (path, options)
);

readFile filename = (
  attemptP \() -> Deno.readTextFile filename
);

writeFile filename data = (
  attemptP \() -> Deno.writeTextFile (filename, data)
);
