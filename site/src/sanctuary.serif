import sanctuary from "sanctuary";

export default default;

kebab-case s = s.replace(
  (new RegExp "(?!\\b)[A-Z0-9]" "g"),
  c => "-" + c.toLowerCase()
)

prime s = s.replaceAll("_", "'")

default =
  (Object.fromEntries ((Object.entries sanctuary.unchecked).map entry => #[(prime (kebab-case entry[0])) entry[1]]))
