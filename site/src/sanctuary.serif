import default as sanctuary from "sanctuary"

export default default

kebab-case = (.replace (new RegExp "(?!\\b)[A-Z0-9]" "g")
                       (c -> "-" + (.toLowerCase c))
                       _)

prime = (.replace "_" "'" _)

default =
  (Object.fromEntries (.map (entry -> #[(Symbol.for (prime (kebab-case entry[0]))) entry[1]])
                            (Object.entries sanctuary.unchecked)))
