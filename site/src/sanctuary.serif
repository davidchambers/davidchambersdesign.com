import sanctuary from "sanctuary";

export default default;

kebab-case = ,(_.replace (new RegExp "(?!\\b)[A-Z0-9]" "g")
                         (c -> "-" + ,(c.toLowerCase)))

prime = ,(_.replaceAll "_" "'")

default =
  (Object.fromEntries ((Object.entries sanctuary.unchecked).map (entry -> #[(Symbol.for (prime (kebab-case entry[0]))) entry[1]])))
