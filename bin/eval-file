#!/usr/bin/env node

'use strict';

const {unchecked: {either}} = require ('sanctuary');

const baseEnv = require ('../src/base.js');
const import_ = require ('../src/import.js');


either (e => { throw e; })
       (x => { if (typeof x === 'string') process.stdout.write (x); })
       (import_ (baseEnv) (process.argv[2]));
